#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jeff/github/svelte-test/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/eslint@7.32.0/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/node_modules:/home/jeff/github/svelte-test/node_modules:/home/jeff/github/node_modules:/home/jeff/node_modules:/home/node_modules:/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/@typescript-eslint+parser@5.10.1_eslint@7.32.0+typescript@4.5.5/node_modules/@typescript-eslint/parser/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/@typescript-eslint+parser@5.10.1_eslint@7.32.0+typescript@4.5.5/node_modules/@typescript-eslint/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/@typescript-eslint+parser@5.10.1_eslint@7.32.0+typescript@4.5.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jeff/github/svelte-test/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/eslint@7.32.0/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/node_modules:/home/jeff/github/svelte-test/node_modules:/home/jeff/github/node_modules:/home/jeff/node_modules:/home/node_modules:/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/@typescript-eslint+parser@5.10.1_eslint@7.32.0+typescript@4.5.5/node_modules/@typescript-eslint/parser/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/@typescript-eslint+parser@5.10.1_eslint@7.32.0+typescript@4.5.5/node_modules/@typescript-eslint/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/@typescript-eslint+parser@5.10.1_eslint@7.32.0+typescript@4.5.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@7.32.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@7.32.0/node_modules/eslint/bin/eslint.js" "$@"
fi
