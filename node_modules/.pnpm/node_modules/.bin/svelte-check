#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jeff/github/svelte-test/node_modules/.pnpm/svelte-check@2.4.0_svelte@3.46.3/node_modules/svelte-check/bin/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/svelte-check@2.4.0_svelte@3.46.3/node_modules/svelte-check/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/svelte-check@2.4.0_svelte@3.46.3/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/node_modules:/home/jeff/github/svelte-test/node_modules:/home/jeff/github/node_modules:/home/jeff/node_modules:/home/node_modules:/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/node_modules/svelte-check/bin/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/node_modules/svelte-check/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jeff/github/svelte-test/node_modules/.pnpm/svelte-check@2.4.0_svelte@3.46.3/node_modules/svelte-check/bin/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/svelte-check@2.4.0_svelte@3.46.3/node_modules/svelte-check/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/svelte-check@2.4.0_svelte@3.46.3/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/node_modules:/home/jeff/github/svelte-test/node_modules:/home/jeff/github/node_modules:/home/jeff/node_modules:/home/node_modules:/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/node_modules/svelte-check/bin/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/node_modules/svelte-check/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
