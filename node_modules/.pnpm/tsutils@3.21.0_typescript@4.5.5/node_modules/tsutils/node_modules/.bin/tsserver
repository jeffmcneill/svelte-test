#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jeff/github/svelte-test/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/bin/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/typescript@4.5.5/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/node_modules:/home/jeff/github/svelte-test/node_modules:/home/jeff/github/node_modules:/home/jeff/node_modules:/home/node_modules:/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/tsutils@3.21.0_typescript@4.5.5/node_modules/tsutils/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/tsutils@3.21.0_typescript@4.5.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jeff/github/svelte-test/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/bin/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/typescript@4.5.5/node_modules/typescript/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/typescript@4.5.5/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/node_modules:/home/jeff/github/svelte-test/node_modules:/home/jeff/github/node_modules:/home/jeff/node_modules:/home/node_modules:/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/tsutils@3.21.0_typescript@4.5.5/node_modules/tsutils/node_modules:/home/jeff/github/svelte-test/node_modules/.pnpm/tsutils@3.21.0_typescript@4.5.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.5.5/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.5.5/node_modules/typescript/bin/tsserver" "$@"
fi
